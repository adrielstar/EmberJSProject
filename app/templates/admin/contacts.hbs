<h1>Contacts</h1>

<table class="table table-hover">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#ID</th>
      <th scope="col">E-mail</th>
      <th scope="col">Message</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |contact|}}
      <tr>
        <th>{{contact.id}}</th>
        <td>{{contact.email}}</td>
        <td>{{contact.message}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>